{
    "pdf": {
        "name": "BEYONDCarts_de-DE",
        "noStdin": true,
        "content": [
            {
                "files": [
                    "**.md",
                    "**.yml"
                ],
                "exclude": [
                    "**/obj/**",
                    "**/includes/**"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "**/*.png",
                    "**/*.jpg",
                    "**/*.gif",
                    "**/*.svg"
                ],
                "exclude": [
                  "**/obj/**",
                  "**/includes/**"
                ],
                "src": "../../logos/carts"
            },
            {
                "files": [
                  "**/*.png",
                  "**/*.jpg",
                  "**/*.gif",
                  "**/*.svg"
                ],
                "exclude": [
                  "**/obj/**",
                  "**/includes/**"
                ]
              }
        ],
        "overwrite": [],
        "wkhtmltopdf": {
            "additionalArguments": "--enable-local-file-access"
        },
        "dest": "../../_site/pdf/",
        "template": ["default","../../templates/pdf-de"]
    }
}